
/%
procedure aaa() return integer
{	
	var a, b: integer;
	var c, d: boolean;

	a = 5;
	b = 10;
	c = true;
	d = !false;

	return --------a*10;
}


procedure bbb(k:integer) return integer
{	
	var a,b: integer;

	b = 10;
	a = a + 10;

	return k;
}

procedure Main() return boolean
{	
	var a,b: integer;
	var c,d: boolean;

	a = -5;
	b = a;
	c = true;
	d = false;

	if(false) {
		a = 1;
	}
	else {
		a = 2;
	}

	a = bbb(3);

	c = d;
	d = true; 
	return !!!d;
}
%/

	procedure Main() return integer {
		/% var s : string[10]; %/

		var a : intptr;
		var b,c: integer;

		/% s = "abc"; %/
		b = 12345678;
		a = &b;
		c = ^a;


		return c;
	}

