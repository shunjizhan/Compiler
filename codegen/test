
/%
procedure aaa() return integer
{	
	var a, b: integer;
	var c, d: boolean;

	a = 5;
	b = 10;
	c = true;
	d = !false;

	return --------a*10;
}

procedure bbb() return integer
{	
	var a,b: integer;

	b = 10;
	a = aaa();
	a = a + 10;

	return a;
}
%/

procedure Main() return integer
{	
	var a,b: integer;
	var c: boolean;

	a = -5;
	b = a;
	c = true;

	if(a >= -5) {
		a = 1;
	}
	else {
		a = 2;
	}

	return a;
}

